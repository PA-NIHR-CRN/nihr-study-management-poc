{
  "AWS": {
    "Region": "eu-west-2"
  },
  "StudyManagementApi": {
    "JwtBearer": {
      "ValidateAudience": false,
      "ValidateIssuerSigningKey": true,
      "Authority": "<required issuer value>",
      "JwtBearerOverride": {
        "OverrideEvents": false,
        "ClaimsOverride": []
      }
    },
    "Data": {
      "ConnectionString": "<db connection string here>"
    }
  },
  "StudyManagement": {
    "DefaultRoleName": "<default role name here>",
    "DefaultLocalSystemName": "<default system name here>"
  },
  "MessageBus": {
    "Topic": "<Kafka topic name>",
    "BootstrapServers": "<Kafka bootstrap server list>"
  },
  "S3": {
    "BucketName": "<optional S3 bucket name. If specified study events will be published to S3. If omitted, messages will be published to Kafka.>",
    "S3FileKeyPrefix": "<optional S3 object key prefix>"
  }
}